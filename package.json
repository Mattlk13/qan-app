{
  "name": "qan-app",
  "version": "v1.2.0",
  "license": "AGPL-3.0",
  "angular-cli": {},
  "scripts": {
    "ng": "PATH=$(npm bin):$PATH; ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "server": "PATH=$(npm bin):$PATH; webpack-dev-server --config webpack.config.js",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build:navless": "ng b -bh '/qan/' -pr -aot -op dist/qan-app --environment=navless",
    "build:prod": "ng b -bh '/qan/' -pr -aot -op dist/qan-app --environment=prod",
    "build:demo": "ng b -bh '/qan/' -pr -aot -op dist/qan-app --environment=demo",
    "build:dev": "ng b -bh '/qan/' -pr -aot -op dist/qan-app",
    "pack": "cd dist/; tar -czf qan-app-`date +%Y%m%dT%H%m%S`.tar.gz qan-app",
    "clean": "rm -r dist/*"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "@types/jszip": "^3.1.2",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.5.1",
    "d3": "^4.11.0",
    "font-awesome": "4.7.0",
    "highlight.js": "9.12.0",
    "jszip": "^3.1.5",
    "moment": "^2.19.1",
    "numeral": "2.0.6",
    "rxjs": "^5.5.2",
    "ts-helpers": "1.1.2",
    "vkbeautify": "^0.99.3",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "5.0.0",
    "@types/d3": "4.11.0",
    "@types/highlight.js": "9.1.10",
    "@types/jasmine": "2.6.2",
    "@types/node": "8.0.47",
    "@types/numeral": "0.0.22",
    "@types/vkbeautify": "0.99.2",
    "codelyzer": "4.0.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-remap-istanbul": "0.6.0",
    "node-sass": "4.5.3",
    "protractor": "5.2.0",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.6",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typescript": "2.6.1"
  }
}
