{
  "name": "qan-app",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "node --max-old-space-size=4096 ./node_modules/.bin/ng serve --proxy-config proxy.conf.json",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "ng b -bh '/qan2/' -pr -aot -op dist/qan-app2 -prod",
    "build:prod": "node --max-old-space-size=4096 ./node_modules/.bin/ng b -bh '/qan2/' -pr -aot -op dist/qan-app2 -prod",
    "pack": "cd dist/; tar -czf qan-app2-`date +%Y%m%d%H%m%S`.tar.zg qan-app2"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.1.1",
    "@angular/compiler": "^4.1.1",
    "@angular/core": "^4.1.1",
    "@angular/forms": "^4.1.1",
    "@angular/http": "^4.1.1",
    "@angular/platform-browser": "^4.1.1",
    "@angular/platform-browser-dynamic": "^4.1.1",
    "@angular/router": "^4.1.1",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.25",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "d3": "^4.8.0",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.11.0",
    "moment": "^2.18.1",
    "numeral": "^2.0.6",
    "rxjs": "^5.3.1",
    "ts-helpers": "^1.1.2",
    "vkbeautify": "^0.99.2",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.2",
    "@angular/compiler-cli": "^4.1.1",
    "@types/d3": "^4.8.0",
    "@types/highlight.js": "^9.1.9",
    "@types/jasmine": "2.5.47",
    "@types/node": "^7.0.18",
    "@types/numeral": "0.0.20",
    "@types/moment": "^2.13.0",
    "node-sass": "^4.5.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "@types/vkbeautify": "^0.99.1",
    "codelyzer": "~3.0.1",
    "jasmine-core": "2.6.1",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.6.0",
    "karma-chrome-launcher": "^2.1.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-remap-istanbul": "^0.6.0",
    "protractor": "~5.1.1",
    "ts-node": "3.0.4",
    "tslint": "^5.2.0",
    "typescript": "~2.3.2"
  }
