{
  "name": "qan-app",
  "version": "v1.2.0",
  "license": "AGPL-3.0",
  "angular-cli": {},
  "scripts": {
    "ng": "PATH=$(npm bin):$PATH; ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "server": "PATH=$(npm bin):$PATH; webpack-dev-server --config webpack.config.js",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build:navless": "ng b -bh '/qan2/' -pr -aot -op dist/qan-app2 --environment=navless",
    "build:prod": "ng b -bh '/qan2/' -pr -aot -op dist/qan-app2 --environment=prod",
    "build:demo": "ng b -bh '/qan2/' -pr -aot -op dist/qan-app2 --environment=demo",
    "build:dev": "ng b -bh '/qan2/' -pr -aot -op dist/qan-app2",
    "pack": "cd dist/; tar -czf qan-app2-`date +%Y%m%dT%H%m%S`.tar.gz qan-app2",
    "clean": "rm -r dist/*"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.2.3",
    "@angular/compiler": "^4.2.3",
    "@angular/core": "^4.2.3",
    "@angular/forms": "^4.2.3",
    "@angular/http": "^4.2.3",
    "@angular/platform-browser": "^4.2.3",
    "@angular/platform-browser-dynamic": "^4.2.3",
    "@angular/router": "^4.2.3",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.26",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "d3": "^4.9.1",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.12.0",
    "moment": "^2.18.1",
    "numeral": "^2.0.6",
    "rxjs": "^5.3.1",
    "ts-helpers": "^1.1.2",
    "vkbeautify": "^0.99.2",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.3",
    "@angular/compiler-cli": "^4.2.4",
    "@types/d3": "^4.9.0",
    "@types/highlight.js": "^9.1.9",
    "@types/jasmine": "2.5.52",
    "@types/node": "^8.0.1",
    "@types/numeral": "0.0.21",
    "@types/moment": "^2.13.0",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "@types/vkbeautify": "^0.99.1",
    "codelyzer": "~3.1.1",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-remap-istanbul": "^0.6.0",
    "protractor": "~5.1.2",
    "ts-node": "3.1.0",
    "tslint": "^5.4.3",
    "typescript": "~2.3.4"
  }
}
